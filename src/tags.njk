---
pagination:
  data: collections
  size: 1
  alias: tag
  filter:
  - all
  - post
  - posts
  - postFeed
permalink: /tags/{{ tag }}/
---
{% extends "layouts/base.njk" %}
{% set postListTitle %}
  Posts filed under "{{ tag|capitalize }}"
{% endset %}
{% set titleLevel = 1 %}
{% set postListItems = collections[tag] %}

{% block content %}
  {% include "partials/components/post-list.njk" %}
{% endblock %}
